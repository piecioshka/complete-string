    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/piecioshka/complete-string.git"
    },
    "engines": {
        "node": ">=14"
    },
    "files": [
        "dist",
        "types",
        "index.js",
        "package.json",
        "LICENSE",
        "README.md",
        "!**/*.spec.*"
    ],
    "keywords": [
        "complete",
        "string",
        "padStart",
        "ES2017"
